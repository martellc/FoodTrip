package com.foodtrip.ftmodeldb.repo;

import org.springframework.data.graph.neo4j.repository.GraphRepository;

import com.foodtrip.ftmodeldb.model.Movie;
import com.foodtrip.ftmodeldb.model.Person;

public interface MovieRepository extends GraphRepository<Movie> {

	  @Query("start movie={0} match m<-[person:ACTS_IN]-user return person")
	  Iterable<Person> getPersons(Movie movie);

	}

